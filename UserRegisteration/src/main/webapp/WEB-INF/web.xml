<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
                             http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>com.example.RegistrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewUserServlet</servlet-name>
        <servlet-class>com.example.ViewUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewUserServlet</servlet-name>  <!-- Corrected this line -->
        <url-pattern>/ViewUsersServlet</url-pattern>
    </servlet-mapping>

</web-app>

